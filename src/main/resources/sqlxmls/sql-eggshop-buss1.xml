<sqls>

	<sql name="BUSS005">
	<![CDATA[
delete from ES_ORDER_ITEM where ORDER_UC = :ORDER_UC 
	]]>
	</sql>

	<sql name="BUSS011">
	<![CDATA[
SELECT U.UNIQUE_CODE
  FROM SYS_USER U, SYS_USERINFO_WX W
 WHERE U.UNIQUE_CODE = W.UNIQUE_CODE
   AND W.OPENID = :OPENID
	]]>
	</sql>

	<sql name="BUSS015">
	<![CDATA[
SELECT * FROM SYS_USERINFO_WX  WHERE UNIQUE_CODE = :UNIQUE_CODE
	]]>
	</sql>
</sqls>